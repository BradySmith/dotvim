snippet dtie
	DROP TABLE ${1:tablename} IF EXISTS;

snippet dvie
	DROP VIEW ${1:viewname} IF EXISTS;

snippet ddie
	DROP DOMAIN ${1:domainname} IF EXIST;

snippet ctfw
	CREATE TABLE ${1:tablename}
	FROM ${2:tablename}
	WHERE ${3:att1}
	;

snippet cva
	CREATE VIEW ${1:viewname} AS

snippet sfw
	SELECT ${1:attributes}
	FROM ${2:tablenames}
	WHERE ${3:conditions}
	;
